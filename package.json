{
  "name": "ramdasauce",
  "version": "1.1.0",
  "description": "Utilities for Ramda.",
  "repository": {
    "type": "git",
    "url": "https://github.com/skellock/ramdasauce.git"
  },
  "main": "./dist/ramdasauce.js",
  "scripts": {
