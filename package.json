    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-stage-2": "^6.13.0",
    "docdash": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rollup": "^2.3.0",
    "jsdoc": "^3.4.0",
    "nyc": "^7.1.0",
    "rollup": "^0.31.2",
